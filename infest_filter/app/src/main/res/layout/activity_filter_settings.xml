    	android:layout_height="match_parent"
        android:text="@string/string_settings_preference_black"
        android:textSize="@dimen/dimen_settings_perference_title_size"
        android:textColor="@color/color_small_black"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/dimen_settings_perference_paddingleft"
        android:clickable="true"/>
        <com.kyleduo.switchbutton.SwitchButton
        android:id="@+id/id_settings_blacklist"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dimen_settings_perference_switchbutton"
        android:layout_marginRight="@dimen/dimen_settings_perference_switchbutton"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        style="@style/MaterialDesignStyle"
            android:checked="false" />
    </RelativeLayout>
    <TextView 
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:layout_marginLeft="@dimen/dimen_settings_perference_divider_margin"
        android:layout_marginRight="@dimen/dimen_settings_perference_divider_margin"
        android:background="@color/color_settings_list_divider"/>
