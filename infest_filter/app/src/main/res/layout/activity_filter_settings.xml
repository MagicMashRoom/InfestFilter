    		android:layout_height="match_parent"
        	android:text="@string/string_settings_preference_subtitle_open"
        	android:textSize="@dimen/dimen_settings_subtitle_size"
        	android:textColor="@color/color_settings_subtitle_color"
        	android:gravity="center_vertical"
        	android:paddingTop="@dimen/dimen_settings_subtitle_paddingtop"
        	android:paddingLeft="@dimen/dimen_settings_subtitle_paddingleft"
        	android:clickable="true"/>
        </LinearLayout>
